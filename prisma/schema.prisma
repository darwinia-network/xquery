// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator typegraphql {
  provider = "typegraphql-prisma"
}

// Model definitions


model Message {
  id    Int    @id @default(autoincrement())
  key String
  srcTx String
  srcBlockNumber String?
  srcSender String?
  tgtTx String?
}



model LcmpMessage {
  messageId String @id // chain+laneId+nonce
  sourceChain String
  targetChain String
  sourceCursor String   @default("")
  acceptedHash String   @default("")
  dispatchHash String   @default("")
  deliveredHash String  @default("")
  dispatchResult String @default("")
  deliveredResult String @default("")
  createdAt DateTime @default(now())
  updateAt DateTime?
  acceptedTime String  @default("")
  dipatchTime String   @default("")
  deliveredTime String @default("")
  
}




